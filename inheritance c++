#include <iostream>

using namespace std;
//syantax of inheritate is = : 
class Animal {
  public:
    void eat() {
      cout << "I am eating" << endl;
    }
};

class Dog : public Animal {
  public:
    void bark() {
      cout << "Woof!" << endl;
    }
};
//calling functions
int main() {
  Dog myDog;
  myDog.eat();
  myDog.bark();
  return 0;
}
