#include <iostream>
using namespace std;

int main() {
    // Initialize an array of size 100 to hold numbers from 101 to 200
    int arr[100];

    for (int i = 0; i < 100; i++) {
        arr[i] = i + 101;
    }
    for (int i = 0; i < 100; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}
